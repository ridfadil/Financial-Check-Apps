<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dua.property.analisis.analisiproperty">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.IncomeActivity"
            android:theme="@style/AppThemeNoActionBar"/>
        <activity android:name=".activity.PassiveIncomeActivity"
            android:theme="@style/AppThemeNoActionBar"/>
        <activity android:name=".activity.SpendingActivity"
            android:theme="@style/AppThemeNoActionBar"/>
        <activity android:name=".activity.SpendingMothActivity"
            android:theme="@style/AppThemeNoActionBar"/>
        <activity android:name=".activity.UserProfilActivity"
            android:theme="@style/AppThemeNoActionBar"></activity>
    </application>

</manifest>